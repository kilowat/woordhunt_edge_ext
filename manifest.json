{
  "name": "Woordhunt audio reader",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "A Chromium extension",
  "icons": {
    "16": "icons/wh16x16.png",
    "32": "icons/wh32x32.png",
    "48": "icons/wh48x48.png",
    "128": "icons/wh128x128.png"
},
"browser_action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
        "matches": [
          "<all_urls>"
        ],
        "js": ["js/index.js"]
    }
  ]
}